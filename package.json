{
  "name": "mpstats-academy-mvp",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "test:e2e": "turbo run test:e2e",
    "db:push": "turbo run db:push --filter=@mpstats/db",
    "db:migrate": "turbo run db:migrate --filter=@mpstats/db",
    "db:generate": "turbo run db:generate --filter=@mpstats/db",
    "db:studio": "turbo run db:studio --filter=@mpstats/db",
    "clean": "turbo run clean && rm -rf node_modules"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "prettier": "^3.2.5",
    "tsx": "^4.21.0",
    "turbo": "^2.3.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  }
}
